      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <%= yield(:header) %>

    <%= csrf_meta_tags %>
  </head>
  <body class="container">
    <% if current_user %>
      <a href="/rapidfire">Home</a>
      <a href="/patient/family_tree">Family Tree</a>
		  <a href="/users/sign_out" class="pull-right">Sign out</a>
