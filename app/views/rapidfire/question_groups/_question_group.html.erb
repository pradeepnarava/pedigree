<tr id= "question_group_<%= question_group.id %>">
  <td>
    <% if can_administer? %>
      <%= link_to question_group.name, question_group_questions_path(question_group) %>
    <% else %>
      <%= question_group.name %>
    <% end %>
  </td>
  <td class="col-md-6">
    <% if can_administer? %>
      <%= request.base_url%>/patient/patient_survey?qid=<%=question_group.id%>
      <!--<li><%= request.original_url%>/question_groups/<%=question_group.id%>/answer_groups/new</li>-->
    <% else %>
      <%= link_to "Answer Questions", new_question_group_answer_group_path(question_group) %>
    <% end %>
    <%# if can_administer? %>
      <!-- <li><%= link_to "Results", [:results, question_group] %></li>
      <li><%= link_to "Delete", [question_group], method: :delete %></li>-->
    <%# end %>
  </td>
</tr>
